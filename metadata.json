{
  "name": "qtechnologies/packages",
  "version": "1.0.2",
  "author": "Q-Technologies",
  "summary": "Puppet module to manage the easy installation of packages across hiera",
  "license": "Apache-2.0",
  "tags": ["packages","software","rpm","deb","macports"],
  "source": "https://github.com/Q-Technologies/puppet-packages.git",
  "project_page": "https://github.com/Q-Technologies/puppet-packages",
  "issues_url": "https://github.com/Q-Technologies/puppet-packages/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.14.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem":"Darwin",
      "operatingsystemrelease": [
        "15"
      ]
    },
    {
      "operatingsystem":"Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    },
    {
      "operatingsystem":"RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [ 
        "11",
        "12"
     ]
    }
  ],
  "data_provider": "hiera"
}

